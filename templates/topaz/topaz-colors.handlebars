.top-bar {
  background-color: {{topbar.hex}}; }

.top-bar .text-right a{
  color: {{topbarIcons.hex}};
  border-right:1px solid #444; }

.top-bar .text-right a:hover,
.top-bar .text-right a:focus {
  color: {{topbarIconsHover.hex}}; }

.socials-simple li a {
  color: {{topbarIcons.hex}};
  font-size: 15px; }

.socials-simple li a:hover {
  color: {{topbarIconsHover.hex}}; }

.navbar-default .navbar-nav > .active > a, .navbar-default .navbar-nav > .active > a:hover, .navbar-default .navbar-nav > .active > a:focus {
  color: {{primary.hex}};}

.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus {
  color: {{primary.hex}}; }

.navbar-brand {
  color: #000 !important; }

.navbar-default {
  background-color: {{header.rgba}}; }

.navbar-default .navbar-nav > li > a {
  color: {{nav.hex}};}

.navbar-default .navbar-nav > li > a:hover {
  color: {{primary.hex}}; }

.tp-caption.modern_big {
  color:#fff;
  background-color:#de543e;
}

.tp-caption.medium_bg_check {
  color:rgb(255, 255, 255);
  background-color:rgb(52, 73, 94);
  border-color:rgb(255, 214, 88);
}

.left-tile-text,
.caption.title-2,
.caption.text,
.text-center-top,
.text-center-btm,
.rev-buttons a,
.footer-col h3 {
  color: {{addressText.hex}};
}
.rev-buttons .border-white:hover {
color:#333;
}

.btn-theme-bg {
  border-color: {{primary.hex}};
  color: #fff;
  background-color: {{primary.hex}};}

.border-white, .btn-theme-bg:hover {
  background-color: #333;
  border-color: #333; }

.rev-buttons .btn-theme-bg:hover {
color:#fff; }


.colored-boxed.green i {
  color: #3F6201;
  border-color: #3F6201; }

.colored-boxed.dark i {
  color: #333;
  border-color: #333; }

.colored-boxed.blue i {
  color: {{primary.hex}};
  border-color: {{primary.hex}}; }

.colored-boxed.red i {
  color: #cb2027;
  border-color: #cb2027; }

#footer {
  background:{{footer.hex}} none repeat scroll 0 0; }

#footer p {
  color:{{addressText.hex}}; }

.footer-btm {
  background-color:{{copyright.hex}};
  color:{{copyrightText.hex}}; }
