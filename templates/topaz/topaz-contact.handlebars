<div class="container">
	<div id="map-canvas">
	<iframe src="" width="100%" height="300" frameborder="0" style="border:0"></iframe></div>
</div>
<div class="divide60"></div>
<div class="container">
	<div class="row">
		<div class="col-md-6 margin30">
			<h3 class="heading">Contact Us</h3>
			<!--fck-->
			fck_one.htm
			<div class="divide20"></div>
			<!--form--> 
			contact_form.htm				
		</div>
				
		<div class="col-md-6 col-sm-12">
			<h3 class="heading">Contact info</h3>
			<ul class="list-unstyled contact contact-info">
				<li><p><strong><i class="fa fa-map-marker"></i> Address:</strong> {{address1}} {{address2}} </p></li> 
				<li><p><strong><i class="fa fa-envelope"></i> Mail Us:</strong> <a href="mailto:{{{email}}}">{{email}} </a></p></li>
				<li> <p><strong><i class="fa fa-phone"></i> Phone:</strong> {{phone}} </p></li>
				<li> <p><strong><i class="fa fa-clock-o"></i> Hours:</strong> Monday - Friday - 08:00 - 8:00 </p></li>
			</ul>
			<div class="divide40"></div>
			{{#if social}}
			<h4>Get social</h4>
			<ul class="list-inline social-1">
			{{#social}}
				{{#if link}}
				<li><a href="{{{link}}}" target="_blank"><i class="fa fa-{{{icon}}}"></i></a></li>
				{{/if}}
			{{/social}}
			</ul>
			{{/if}}
		</div>
	</div>
</div>