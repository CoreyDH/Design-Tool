{{#navigation}}
{{#unless subtitle}}
<li><a href="{{{link}}}"><span class="link-item">{{title}}</span></a></li>
{{/unless}}
{{#if subtitle}}
<li class="dropdown">
	<a href="#" class="dropdown-toggle" data-toggle="dropdown"><span class="link-item">{{title}} <i class="fa fa-angle-down"></i></span></a>
		<ul class="dropdown-menu sub-menu" role="menu">
		{{#subtitle}}
			<li><a href="{{{link}}}">{{title}}</a></li>
		{{/subtitle}}
		</ul>
	</li>
{{/if}}
{{/navigation}}
