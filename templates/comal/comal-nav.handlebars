{{#navigation}}
{{#unless subtitle}}
<li class="nav-{{page}}"><a href="{{{link}}}">{{title}}</a></li>
{{/unless}}
{{#if subtitle}}
<li class="dropdown nav-events"><a class="dropdown-toggle" data-toggle="dropdown" href="#">{{title}} <b class="caret"></b></a>
	<ul class="dropdown-menu">
	{{#subtitle}}
		<li class="nav-{{page}}"><a href="{{{link}}}">{{title}}</a></li>
	{{/subtitle}}
	</ul>
</li>
{{/if}}
{{/navigation}}